schema {
    query: QueryType
    mutation: MutationType
}

type QueryType {
    category(key: ID!): Category
    categories : [ Category ]
    snippet(key: ID!): Snippet
    snippets : [ Snippet ]
}

type MutationType {
    deleteCategory(key: ID!): Boolean
    deleteSnippet(key: ID!): Boolean
    category(name: String!): Category
}

type Snippet {
    key: ID!
    category: String!
    title: String!
    body: String!
}

type Category {
    key: ID!
    name: String!
}